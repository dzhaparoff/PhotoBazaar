    <i class="tags icon"></i>
    Теги
  </a>
  <!--<div class="ui horizontal divider section inverted">
    Actions
  </div>-->
  <div class="ui divider hidden"></div>
  <a class="item no_pre">
    <i class="setting icon"></i>
    Настройки
  </a>
  <a class="item" href="<%= destroy_user_session_path %>" data-method="delete">
    <i class="sign out icon"></i>
    Выход
  </a>
</div>
<div class="pusher"> 
  <div class="ui black massive launch right attached fixed button">
      <i class="content icon"></i>  
  </div>
<%= content_for?(:content) ? yield(:content) : yield %>
</div>
</body>
</html>